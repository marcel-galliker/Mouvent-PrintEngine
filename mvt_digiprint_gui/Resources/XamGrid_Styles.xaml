            <Setter.Value>
                <ControlTemplate TargetType="{x:Type GridSplitter}">
                    <Border Height="{TemplateBinding Height}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}">
                        <TextBlock FontSize="{DynamicResource MvtFontSize}" Foreground="#ff303030" 
