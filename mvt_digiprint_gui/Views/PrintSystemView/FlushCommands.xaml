<UserControl x:Class="RX_DigiPrint.Views.PrintSystemView.FlushCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RX_DigiPrint.Views.PrintSystemView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox Grid.Column="0" Header="Flush" Style="{StaticResource RxGroupBox}">
        <Grid>
            <StackPanel>
                <CheckBox Content="Flush C1" Click="Flush_C1_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Flush_C1"/>
                <TextBlock/>
                <CheckBox Content="Flush C2" Click="Flush_C2_Clicked"  Style="{DynamicResource RxCommandButton}" x:Name="Button_Flush_C2"/>
                <TextBlock/>
                <CheckBox Content="Flush C3" Click="Flush_C3_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Flush_C3"/>
                <TextBlock/>
                <CheckBox Content="Flush C4" Click="Flush_C4_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Flush_C4"/>
            </StackPanel>

        </Grid>
    </GroupBox>
</UserControl>
