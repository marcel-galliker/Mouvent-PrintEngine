<UserControl x:Class="RX_DigiPrint.Views.PrintSystemView.Conditioner_Reset"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RX_DigiPrint.Views.PrintSystemView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <GroupBox Grid.Column="0" Header="Test Table Seon Commands" Style="{StaticResource RxGroupBox}">


            <StackPanel>
                <CheckBox Content="Reset_Cond" Grid.Row ="0" Grid.Column="0" Click="Reset_Conditioner"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Cond_Reset"/>
                <TextBlock/>
                <CheckBox Content="Jetting" Grid.Row ="1" Grid.Column="0" Click="Jetting"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Jetting"/>
                <TextBlock/>
                <CheckBox Content="Jetting Abort" Grid.Row ="1" Grid.Column="0" Click="Jetting_Abort"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Jetting_Abort"/>
            </StackPanel>

        </GroupBox>
    </Grid>
</UserControl>
