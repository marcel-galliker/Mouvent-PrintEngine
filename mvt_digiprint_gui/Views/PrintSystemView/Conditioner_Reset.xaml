<UserControl x:Class="RX_DigiPrint.Views.PrintSystemView.Conditioner_Reset"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RX_DigiPrint.Views.PrintSystemView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox Grid.Column="0" Header="Conditioner Reset" Style="{StaticResource RxGroupBox}">
        <Grid>
            <StackPanel>
                <CheckBox Content="Reset_Cond" Click="Reset_Conditioner"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Cond_Reset"/>
            </StackPanel>

        </Grid>
    </GroupBox>
</UserControl>
