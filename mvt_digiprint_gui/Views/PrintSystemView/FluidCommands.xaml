<UserControl x:Class="RX_DigiPrint.Views.PrintSystemView.FluidCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RX_DigiPrint.Views.PrintSystemView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox Grid.Column="0" Header="Leak Test" Style="{StaticResource RxGroupBox}">
        <Grid>
            <StackPanel>
                <CheckBox Content="Leak Test C1" Click="LeakTest_C1_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Leak_C1"/>
                <TextBlock/>
                <CheckBox Content="Leak Test C2" Click="LeakTest_C2_Clicked"  Style="{DynamicResource RxCommandButton}" x:Name="Button_Leak_C2"/>
                <TextBlock/>
                <CheckBox Content="Leak Test C3" Click="LeakTest_C3_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Leak_C3"/>
                <TextBlock/>
                <CheckBox Content="Leak Test C4" Click="LeakTest_C4_Clicked"   Style="{DynamicResource RxCommandButton}" x:Name="Button_Leak_C4"/>
            </StackPanel>

        </Grid>
    </GroupBox>
</UserControl>
