        <Line Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="4"
            VerticalAlignment="Top" Stroke="WhiteSmoke" StrokeThickness="2"
            X2="{Binding Path=ActualWidth, RelativeSource={RelativeSource Self}}" />

        <Line Grid.Row="9" Grid.Column="1" Grid.ColumnSpan="4"
            VerticalAlignment="Top" Stroke="WhiteSmoke" StrokeThickness="2"
            X2="{Binding Path=ActualWidth, RelativeSource={RelativeSource Self}}" />


        <Border BorderBrush="WhiteSmoke" BorderThickness="2" Grid.Row="0" Grid.Column="1" Grid.RowSpan="15" Grid.ColumnSpan="4"></Border>
        <Border Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="4"  BorderThickness="2" BorderBrush="WhiteSmoke" Background="WhiteSmoke">
            <TextBlock Foreground="#ff303030" FontWeight="SemiBold" FontSize="14" HorizontalAlignment="Center" VerticalAlignment="Center"
                       Text="{Binding ClusterName}">
            </TextBlock>
        </Border>
        <Line Grid.Row="1" Grid.Column="1" Grid.RowSpan="14"
            HorizontalAlignment="Right" Stroke="WhiteSmoke" StrokeThickness="2"
            Y2="{Binding Path=ActualHeight, RelativeSource={RelativeSource Self}}"></Line>
        <Line Grid.Row="1" Grid.Column="2" Grid.RowSpan="14"
            HorizontalAlignment="Right" Stroke="WhiteSmoke" StrokeThickness="2"
            Y2="{Binding Path=ActualHeight, RelativeSource={RelativeSource Self}}"></Line>
        <Line Grid.Row="1" Grid.Column="3" Grid.RowSpan="14"
            HorizontalAlignment="Right" Stroke="WhiteSmoke" StrokeThickness="2"
            Y2="{Binding Path=ActualHeight, RelativeSource={RelativeSource Self}}"></Line>



        <common:MvtButton Grid.Row="1" Grid.Column="1"
                              x:Name="PrintHeadColor0" Style="{DynamicResource Mouvent.Alignment.PrintHeadColorButton}" Margin="10" ></common:MvtButton>
        <common:MvtButton Grid.Row="1" Grid.Column="2"
                              x:Name="PrintHeadColor1" Style="{DynamicResource Mouvent.Alignment.PrintHeadColorButton}" Margin="10" ></common:MvtButton>
        <common:MvtButton Grid.Row="1" Grid.Column="3"
                              x:Name="PrintHeadColor2" Style="{DynamicResource Mouvent.Alignment.PrintHeadColorButton}" Margin="10" ></common:MvtButton>
        <common:MvtButton Grid.Row="1" Grid.Column="4"
                              x:Name="PrintHeadColor3" Style="{DynamicResource Mouvent.Alignment.PrintHeadColorButton}" Margin="10" ></common:MvtButton>


            <common:MvtButton  Style="{StaticResource Mouvent.RoundIconButtonStyle.LightBackgroundInfoButton.SizeL}" Grid.Column="1"
