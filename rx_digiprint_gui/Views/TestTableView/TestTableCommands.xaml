<UserControl x:Class="RX_DigiPrint.Views.TestTableView.TestTableCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="Commands" Style="{StaticResource RxGroupBox}">
            <StackPanel>
                <CheckBox Content="STOP" Click="Stop_Clicked" Style="{DynamicResource RxCommandButton}"/>
                <TextBlock/>
                <CheckBox Content="Reference"         Click="StartRef_Clicked"   Style="{DynamicResource RxCommandButton}"/>
                <TextBlock/>
                <CheckBox Content="Slide To Right"    Click="ScanRight_Clicked"  Style="{DynamicResource RxCommandButton}"/>
                <TextBlock/>
                <CheckBox Content="Slide To Left"     Click="ScanLeft_Clicked"   Style="{DynamicResource RxCommandButton}"/>
            </StackPanel>

        </GroupBox>
    </Grid>
</UserControl>
